{
  "name": "incognito-chain-web-js",
  "version": "1.0.0",
  "description": "Constant-chain Javascript API",
  "main": "build-node/inc.js",
  "module": "build/web/wallet.js",
  "scripts": {
    "start": "node -r ./inc.config",
    "start:dev": "yarn test-account && yarn build",
    "build:wasm": "cd wasm && ./build.sh && cd ..",
    "build:watch": "webpack --config webpack.config.js --watch",
    "build": "webpack --config webpack.config.js --mode production",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "build:verifier": "./lib/verifier/build-wasm.sh && webpack --config verifier-webpack.config.js --mode development",
    "test": "yarn build:wasm; mocha \"test/**/*.spec.js\"",
    "test-account": "clear && yarn build && node ./test/wallet/accountwallet-test",
    "test-portal": "clear && yarn build:dev && node ./test/wallet/portal-test",
    "test-punified": "clear && yarn build:dev && node ./test/wallet/punified-test",
    "build:gomobile-ios": "cd wasm/internal && gomobile bind -x -v -target=ios",
    "build:gomobile-android": "cd wasm/internal && gomobile bind -x -v -target=android",
    "run-test": "clear && node ./test/wallet/accountwallet-test",
    "build:gomobile": "yarn build:wasm && yarn build:gomobile-ios && yarn build:gomobile-android && yarn build",
    "push-code": "git add . && git commit -m 'push code' && git push",
    "fix-permission": "ssh-agent -s && ssh-add ~/.ssh/id_rsa",
    "test-pdex3": "clear && yarn build && node ./test/wallet/pdex3-test",
    "run-test-pdex3": "clear && node ./test/wallet/pdex3-test",
    "build-web": "webpack --config webpack.config.js --mode production --config-name wallet-web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incognitochain/incognito-chain-web-js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/incognitochain/incognito-chain-web-js/issues"
  },
  "homepage": "https://github.com/incognitochain/incognito-chain-web-js#readme",
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@vespaiach/axios-fetch-adapter": "^0.3.0",
    "axios": "^0.19.0",
    "binary-sorted-array": "^1.0.4",
    "bip39": "^3.0.3",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^16.0.0",
    "hdkey": "^2.0.1",
    "lodash": "^4.17.20",
    "regenerator-runtime": "^0.13.7",
    "sha3": "^2.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "^7.12.11",
    "@ledgerhq/hw-transport": "^6.27.8",
    "@ledgerhq/hw-transport-http": "^6.27.8",
    "@ledgerhq/hw-transport-node-hid": "^6.27.8",
    "@ledgerhq/hw-transport-webhid": "^6.27.8",
    "@ledgerhq/hw-transport-webusb": "^6.27.9",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "bufferutil": "^4.0.7",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-bn": "^0.2.1",
    "core-js": "^3.10.2",
    "encoding": "^0.1.13",
    "file-loader": "^6.2.0",
    "fs": "0.0.1-security",
    "hw-app-incognito": "github:incognitochain/ledger-hw-app#dev/fix-transport-http",
    "lodash-webpack-plugin": "^0.11.6",
    "merkle-patricia-tree": "^4.0.0",
    "mocha": "^8.3.2",
    "node-fetch": "2",
    "terser-webpack-plugin": "^1.2.3",
    "utf-8-validate": "^5.0.10",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "worker-loader": "^3.0.8"
  },
  "mocha": {
    "timeout": "1200000"
  }
}
